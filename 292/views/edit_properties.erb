    <h1>List of Properties</h1>
    <table border="1">
      <tr>
        <th>ID #</th>
        <th>Address</th>
        <th>Price</th>
        <th></th>
      </tr>
      <% for property in @properties %>
        <tr>
          <td><%= property.id %></td>
          <td><%= property.address %></td>
          <td><%= property.price %></td>
          <td><a href="/<%= property.id %>">edit</a>
        </tr>
      <% end %>
    </table>
