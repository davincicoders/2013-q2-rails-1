<table border="1">
  <tr>
    <th>ID #</th>
    <th>Address</th>
    <th>Price</th>
    <th></th>
    <th></th>
  </tr>
  <% for property in @properties %>
    <tr>
      <td><%= property.id %></td>
      <td><%= property.address %></td>
      <td><%= property.price %></td>
      <td>
        <%= link_to "Edit", edit_one_path(property) %>
      </td>
      <td>
        <%= form_tag delete_path(property) do %>
          <button name="delete" value="delete">Delete</button>
        <% end %>
      </td>
    </tr>
  <% end %>
  <tr>
    <td colspan="5">
      <%= link_to "Add New Property", add_new_path %>
    </td>
  </tr>
</table>
